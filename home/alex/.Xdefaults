URxvt.perl-ext-common: clipboard
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.font: xft:Source Code Pro:size=10:antialias=true:hinting=true,xft:DejaVu Sans Mono:size=10:antialias=true:hinting=true,xft:Noto Color Emoji
