Xft.dpi: 144

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt.perl-ext-common: clipboard
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.font: xft:Source Code Pro for Powerline:size=12:antialias=true:hinting=true,xft:DejaVu Sans Mono:size=12,xft:Font Awesome 5 Free:size=12,xft:Noto Color Emoji:size=12
URxvt.boldFont: xft:Source Code Pro for Powerline:size=12:antialias=true:hinting=true,xft:DejaVu Sans Mono:size=12,xft:Noto Color Emoji:size=12
URxvt.italicFont: xft:Source Code Pro for Powerline:style=Oblique:size=12:antialias=true:hinting=true,xft:Noto Color Emoji:size=12
URxvt.boldItalicFont: xft:Source Code Pro for Powerline:style=Bold Oblique:size=12:antialias=true:hinting=true,xft:Noto Color Emoji:size=12
URxvt.letterSpace: -2
URxvt.scrollBar: false

URxvt.background:  #2b2b2b
URxvt.foreground:  #a9b7c6
